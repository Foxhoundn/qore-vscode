{
    "name": "qore-vscode",
    "displayName": "qore",
    "description": "Qore Language Support",
    "version": "0.0.1",
    "publisher": "qoretechnologies",
    "engines": {
        "vscode": "^1.5.0"
    },
    "license": "SEE LICENSE IN LICENSE.txt",
    "bugs": {
        "url": "https://github.com/qorelanguage/qore-vscode/issues"
    },
    "homepage": "https://github.com/qorelanguage/qore-vscode/blob/master/README.md",
    "categories": [
        "Languages",
        "Linters",
        "Other"
    ],
    "keywords": [
        "qore",
        "language",
        "programming",
        "programming_language",
        "language_server"
    ],
    "activationEvents": [
        "onLanguage:qore"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [{
            "id": "qore",
            "aliases": ["Qore", "qore"],
            "extensions": [".q",".qm",".qtest",".ql",".qc",".qsd",".qfd",".qwf",".qjob",".qclass",".qconst",".qsm"],
            "configuration": "./qore.configuration.json"
        }],
        "grammars": [{
            "language": "qore",
            "scopeName": "source.qore",
            "path": "./syntaxes/qore.tmLanguage.json"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "update-vscode": "node ./node_modules/vscode/bin/install",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "vscode-languageclient": "^2.6.3"
    }
}
